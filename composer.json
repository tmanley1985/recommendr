{
    "name": "tmanley1985/recommendr",
    "description": "A recommendation engine in PHP",
    "keywords": [
        "tmanley1985",
        "recommendr"
    ],
    "homepage": "https://github.com/tmanley1985/recommendr",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Manley",
            "email": "tmanley1985@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Tmanley1985\\Recommendr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tmanley1985\\Recommendr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
